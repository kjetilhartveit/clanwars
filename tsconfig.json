{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"preserveConstEnums": true,
		"outDir": "compiled/app"
	},
	"include": [
		"app/**/*",
		"typings/tsd.d.ts"
	],
  "exclude": [
		"dist",
    "node_modules",
    "typings/main",
    "typings/main.d.ts"
  ]
}